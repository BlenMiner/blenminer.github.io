{
    "name": "widget-template-vue",
    "version": "1.0.0",
    "description": "Widget Template using Vue.js",
    "main": "index.html",
    "config": {
        "publicPath": null
    },
    "scripts": {
        "postinstall": "node webpack/postInstall.js",
        "test": "mocha test/e2e/bootstrap.js --recursive test --timeout 10000",
        "debugTest": "mocha --inspect-brk test/bootstrap.js --recursive test --timeout 10000",
        "build": "webpack --config ./webpack/webpack.config.prod.js",
        "start": "webpack-dev-server --config ./webpack/webpack.config.dev.js --open",
        "startS3": "webpack-dev-server --config ./webpack/webpack.config.dev.s3.js",
        "debugS3": "node --inspect node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/webpack.config.dev.s3.js",
        "lint": "eslint src/**/*.js",
        "lintFix": "eslint src/**/*.js --fix",
        "lintMark": "markdownlint --config .markdownlint.json --ignore ./node_modules/** ./**/*.md",
        "lintMarkFix": "markdownlint --fix --config .markdownlint.json --ignore ./node_modules/** ./**/*.md"
    },
    "repository": {
        "type": "git",
        "url": "git@itgit.dsone.3ds.com:widget-lab/widget-template-vue.git"
    },
    "author": "widget-lab",
    "license": "MIT",
    "devDependencies": {
        "@auth0/s3": "1.0.0",
        "@babel/core": "7.7.2",
        "@babel/preset-env": "7.7.1",
        "@mdi/font": "4.5.95",
        "babel-loader": "8.3.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "11.0.0",
        "css-loader": "6.7.3",
        "deepmerge": "4.2.2",
        "eslint": "6.7.2",
        "eslint-config-prettier": "6.7.0",
        "eslint-config-vuetify": "0.4.1",
        "eslint-loader": "4.0.2",
        "eslint-plugin-html": "6.0.0",
        "eslint-plugin-json": "2.0.1",
        "eslint-plugin-mocha": "6.2.2",
        "eslint-plugin-vue": "6.0.1",
        "express": "4.17.1",
        "fibers": "4.0.2",
        "file-loader": "6.2.0",
        "highlight.js": "10.4.1",
        "markdownlint": "0.17.2",
        "markdownlint-cli": "0.19.0",
        "material-design-icons-iconfont": "5.0.1",
        "mermaid": "8.4.2",
        "mocha": "6.2.2",
        "puppeteer": "2.0.0",
        "raw-loader": "4.0.2",
        "remarkable": "2.0.0",
        "sass": "1.23.7",
        "sass-loader": "13.2.0",
        "style-loader": "1.3.0",
        "vue": "2.6.10",
        "vue-loader": "17.0.1",
        "vue-template-compiler": "2.6.10",
        "vuetify": "2.1.10",
        "vuetify-loader": "1.9.2",
        "vuex": "3.1.2",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "3.9.0",
        "webpack-merge": "4.2.2"
    },
    "dependencies": {
        "chart.js": "^3.7.1",
        "chartjs-adapter-moment": "^1.0.0",
        "moment": "^2.29.1",
        "xlsx": "^0.18.2"
    }
}
